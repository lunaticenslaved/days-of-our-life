{
  "name": "space-runner",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": " nodemon src/server/index.ts",
    "build": "NODE_ENV=production && rm -rf /dist && tsc -p src/server/tsconfig.json && tsc-alias -p src/server/tsconfig.json && vite build --config ./vite.client.config.ts && vite build --config ./vite.server.config.ts",
    "start": "node dist/server/index.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && prettier .",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix & prettier . --write",
    "deploy": "npx prisma migrate deploy --schema src/server/prisma/schema.prisma && npm run start",
    "storybook": "storybook dev -p 6006",
    "typecheck": "tsc --noEmit",
    "build-storybook": "storybook build",
    "db:add-migration": "sequelize-cli migration:create --name",
    "db:up": "sequelize-cli db:migrate --name",
    "db:down": "sequelize-cli db:migrate:undo --name"
  },
